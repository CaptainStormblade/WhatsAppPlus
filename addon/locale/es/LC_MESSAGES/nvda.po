#: addon\appModules\whatsapp.py:452
msgid "Video call not available"
msgstr "Videollamada no disponible"

#. Accept call
#: addon\appModules\whatsapp.py:455
msgid "Accept call"
msgstr "Aceptar llamada"

#. End or decline call
#: addon\appModules\whatsapp.py:462
msgid ""
"Press \"Decline call\" button  if there is an incoming call or \"End call\" "
"button if a call is in progress"
msgstr ""
"Presionar el botón \"Rechazar llamada\" si hay una llamada entrante, \"o el "
"botón \"finalizar llamada\" si hay una llamada en curso"

#. Mute/unmute the microphone
#: addon\appModules\whatsapp.py:469
msgid "Mute or unmute the microphone"
msgstr "Silenciar o desilenciar el micrófono"

#. Turn on/off the camera
#: addon\appModules\whatsapp.py:481
msgid "Turn the camera on and off"
msgstr "Activar y desactivar la cámara"

#. Press the "Attach media" button
#: addon\appModules\whatsapp.py:493
msgid "Press \"Attach file\" button"
msgstr "Presionar el botón \"Adjuntar\""

#: addon\appModules\whatsapp.py:497 addon\appModules\whatsapp.py:506
#: addon\appModules\whatsapp.py:612
